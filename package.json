{
  "name": "miniplug",
  "description": "Small plug.dj API client for bots and applications, built on Promises",
  "version": "2.0.0-beta.0",
  "author": "RenÃ©e Kooi <renee@kooi.me>",
  "bugs": "https://github.com/miniplug/miniplug/issues",
  "contributors": [
    "Burkes (https://github.com/burkes)",
    "Cosmetify (https://github.com/cosmetify)"
  ],
  "dependencies": {
    "bluebirdish": "^1.1.0",
    "debug": "^3.0.0",
    "node-fetch": "^2.1.2",
    "plug-login": "^2.0.0",
    "plug-socket": "^1.0.0"
  },
  "devDependencies": {
    "async-to-gen": "^1.3.3",
    "buble": "^0.19.0",
    "builtin-modules": "^3.0.0",
    "create-error-class": "^3.0.2",
    "glob": "^7.1.2",
    "is-async-supported": "^1.2.0",
    "linear-promise-backoff-queue": "^1.0.0",
    "lint-staged": "^7.2.0",
    "nock": "^9.3.0",
    "npm-run-all": "^4.1.3",
    "plug-message-split": "^1.1.1",
    "pre-commit": "^1.2.2",
    "proxyquire": "^2.0.1",
    "rollup": "^0.61.2",
    "rollup-plugin-buble": "^0.19.0",
    "rollup-plugin-commonjs": "^9.0.0",
    "rollup-plugin-inject": "^2.0.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "standard": "^11.0.0",
    "tap-min": "^1.2.1",
    "tape": "^4.6.3"
  },
  "engines": {
    "node": ">= 4"
  },
  "files": [
    "index.js",
    "index.mjs",
    "docs"
  ],
  "homepage": "https://github.com/miniplug/miniplug",
  "keywords": [
    "plug",
    "plug.dj",
    "plugdj",
    "api",
    "bot",
    "client",
    "promises"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "main": "index.js",
  "module": "index.mjs",
  "pre-commit": {
    "run": [
      "rebuild",
      "tap:min",
      "lint-staged"
    ]
  },
  "repository": "miniplug/miniplug",
  "scripts": {
    "build": "rollup -c",
    "lint": "standard rollup.config.js src/**/*.js test/**/*.js",
    "lint-staged": "lint-staged",
    "prepare": "npm run build",
    "rebuild": "node has-changed || rollup -c",
    "tap": "tape -r ./test/_init.js test/*.js",
    "tap:min": "npm run tap | tap-min",
    "test": "npm-run-all lint rebuild tap:min",
    "test:ci": "npm-run-all lint rebuild tap"
  },
  "standard": {
    "env": "mocha"
  }
}
